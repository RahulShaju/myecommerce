      <%- include('../partials/userlandingheader.ejs') -%>
      <!-- mini cart start -->
<br><br><br><br>

      <div class="breadcrumb-area bg-dark text-white">
        <div class="container">
          <div class="text-center">
            <ul>
              <li>
                Already a user ? <a href="/login" style="color: red">Login</a>
              </li>
            </ul>
          </div>
        </div>
      </div>
     
      <div class="login-register-area pt-115 pb-120 image-responsive bg-gray text-center" style="background-image:url('assets/images/bg/bg-1.jpg');background-repeat: no-repeat; background-size: contain; background-position: center;"  >
        <div class="container" >
          <div class="row">
            <div class="col-lg-7 col-md-12 ms-auto me-auto">
              <div class="login-register-wrapper" >
                
                <div class="tab-content">
                  <div id="lg2">
                    <div class="login-form-container bg-dark" style="border-radius: 20px; opacity: 0.8;">
                      <div class="login-register-form ">
                        <form action="#" method="post">
                          <h2 style="color: red; font-weight: bold;font-size: 34px;color: white;" class="text-center">Register</h2>
                          <br>
                          <input style="border: solid 1px white ;border-radius: 10px;font-weight: bold; color: white; font-size: 24px;"
                            type="text"
                            name="name"
                            placeholder="Username"
                          />
                          <input style="border: solid 1px white ;border-radius: 10px;font-weight: bold; color: white; font-size: 24px;"
                            type="password"
                            name="password"
                            placeholder="Password"
                          />
                          <input style="border: solid 1px white ;border-radius: 10px;font-weight: bold; color: white; font-size: 24px;"
                            name="email"
                            placeholder="Email"
                            type="email"
                          />
                          <input style="border: solid 1px white ;border-radius: 10px;font-weight: bold; color: white; font-size: 24px;"
                          name="mobile"
                          placeholder="Mobile Number"
                          type="text" required
                        />
                      
                        <% if(typeof message !== 'undefined'){ %>
                          <p style="color: red"><%= message %></p>
                          <% }%>
                            
                          
                          <div class="button-box">
                            <button type="submit"style="border-radius:20px;" >Register</button>
                          </div>
                        </form>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    
     
      
      
      
