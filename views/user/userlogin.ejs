                                        <div class="login-register-form">
                                            <form action="" method="post">
                                                <div class="text-center"><h2 style="color:white; font-weight: bold; font-size: 40px;">Login</h2></div>
                                                <input style="border: solid 1px white ;border-radius: 10px;font-weight: bold; color: white; font-size: 24px;" type="email" name="username" placeholder="Username" required>
                                                <input style="border: solid 1px white;border-radius: 10px;font-weight: bold; color: white; font-size: 24px;" type="password" name="logpwd" placeholder="Password" required>
                                                <% if(typeof message !== 'undefined'){ %>
                                                    <p style="color: rgb(202, 6, 6) ;font-size: 16px;"><%= message %></p>
                                                    <% }%>
                                                <div class="button-box">
                                                    
                                                    <button  type="submit">Login</button>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        
